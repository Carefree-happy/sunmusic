.indexContainer {
  display: flex;
  flex-direction: column;
  align-items: center;
  background: #f0f;
  height: 100%;
}

.avatarUrl {
  width: 200rpx;
  height: 200rpx;
  border-radius: 100rpx;
  margin: 100rpx 0;
}

.userName {
  font-size: 38rpx;
  margin: 100rpx 0;
}

.goStudy {
  width: 400rpx;
  height: 80rpx;
  line-height: 80rpx;
  text-align: center;
  border: 1rpx solid #333;
  border-radius: 40rpx;
}